{
	"name": "suvvybyte-portofolio",
	"private": true,
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"dev": "vite dev --port 3000",
		"build": "vite build",
		"preview": "vite preview --port 3000",
		"prepare": "svelte-kit sync || echo ''",
